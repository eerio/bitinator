# canonical: https://www.
<IfModule mod_rewrite.c>
        RewriteEngine On

        RewriteCond %{HTTP:X-Forwarded-Proto} =http
        RewriteRule (.*) https://%{HTTP_HOST}%{REQUEST_URI} [L,R=303]

        RewriteCond %{HTTP_HOST} ^bitinator\.net [NC]
        RewriteRule (.*) https://www.%{HTTP_HOST}%{REQUEST_URI} [L,R=303]
</IfModule>
